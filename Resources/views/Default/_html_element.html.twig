{% for elm in adminlist.htmlElements %}
    {% if elm.is(position) %}
        {% if item is not defined %}
                {% if elm.isTemplate %}
                    {% include elm.src with {'adminlist':adminlist} %}
                {% else %}
                    {{ render(controller(elm.src ,{'request':app.request,'adminlist':adminlist})) }}
                {% endif %}
        {% else %}
            {% if elm.isTemplate %}
                    {% include elm.src with {'adminlist':adminlist,'item':item} %}
                {% else %}
                    {{ render(controller(elm.src ,{'request':app.request,'adminlist':adminlist,'item':item})) }}
                {% endif %}
        {% endif %}
    {% endif %}
{% endfor %}