{% set quote = '"' %}
{%- for field in configurator.exportFields -%}
{{- quote|raw -}}{{- field.header | trans -}}{{- quote|raw -}}
{%- if not loop.last -%},{%- endif -%}
{%- endfor -%}
{% for entity in iterator %}
{% if entity[0] is defined %}
{% set entity = entity[0] %}
{% else %}
{% set entity = entity %}
{% endif %}
{% for field in configurator.exportFields %}
{% set columnName = field.name %}
{% set template = field.template %}
{% if template is not null and template != 1 and template != "" %}
{{- quote|raw -}}{% include template with {'entity': entity, 'field': field, 'object': adminlist.getValue(item, columnName)} %}{{- quote|raw -}}
{% else %}
{{- quote|raw -}}{{- configurator.getStringValue(entity, columnName) -}}{{- quote|raw -}}
{% endif %}
{%- if not loop.last -%},{%- endif -%}
{% endfor %}
{% endfor %}
