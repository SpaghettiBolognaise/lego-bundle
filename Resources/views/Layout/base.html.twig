<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}{% endblock %}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" href="/bundles/idklego/images/favicon.ico" />
    {% block stylesheet %}
        <!-- Bootstrap 3.3.6 -->
        <link rel="stylesheet" href="/bundles/idklego/adminLte/bootstrap/css/bootstrap.min.css">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="/bundles/idklego/css/jquery-ui.min.css">
        <link rel="stylesheet" href="/bundles/idklego/lib/jqueryuidatetimepicker.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
        <!-- Theme style -->
        <link rel="stylesheet" href="/bundles/idklego/adminLte/dist/css/AdminLTE.min.css">
        <!-- AdminLTE Skins. Choose a skin from the css/skins
             folder instead of downloading all of them to reduce the load. -->
        <link rel="stylesheet" href="/bundles/idklego/adminLte/plugins/select2/select2.css">
        <link rel="stylesheet" href="/bundles/idklego/adminLte/dist/css/skins/_all-skins.min.css">
        <link rel="stylesheet" href="/bundles/idklego/adminLte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
        <link rel="stylesheet" href="/bundles/idklego/lib/jsoneditor/jsoneditor.min.css">
        <link rel="stylesheet" href="/bundles/idklego/lib/codemirror/lib/codemirror.css">
        <link rel="stylesheet" href="/bundles/idklego/lib/treetable/jquery.treetable.css">
        <link rel="stylesheet" href="/bundles/idklego/lib/treetable/jquery.treetable.theme.default.css">
        <link rel="stylesheet" href="/bundles/idklego/lib/gridstack/gridstack.css">

        <link rel="stylesheet" href="/bundles/idklego/lib/datetimepicker/bootstrap-datetimepicker.min.css">
        <link rel="stylesheet" href="/bundles/idklego/css/jsa.css">
        <link rel="stylesheet" href="/bundles/idklego/css/lego.css">
    {%  endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    {% block javascript %}
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <script src="/bundles/idklego/lib/lodash/lodash.js"></script>

        <!-- jQuery 2.2.3 -->
        <script src="/bundles/idklego/adminLte/plugins/jQuery/jquery-2.2.3.min.js"></script>
        <script src="/bundles/idklego/adminLte/plugins/jQueryUI/jquery-ui.min.js"></script>
        <script src="/bundles/idklego/lib/jqueryuidatetimepicker.js"></script>


        <script src="/bundles/idklego/adminLte/plugins/select2/select2.min.js"></script>

        <!-- JsonEditor -->
        <script src="/bundles/idklego/lib/jsoneditor/jsoneditor.min.js"></script>
        <script src="/bundles/idklego/lib/jsoneditor/jsoneditor-human.min.js"></script>
        <script src="/bundles/idklego/lib/codemirror/lib/codemirror.js"></script>
        <script src="/bundles/idklego/lib/codemirror/lib/formatting.js"></script>
        <script src="/bundles/idklego/lib/codemirror/mode/javascript/javascript.js"></script>
        <!-- Bootstrap 3.3.6 -->
        <script src="/bundles/idklego/adminLte/bootstrap/js/bootstrap.min.js"></script>
        <!-- Datetimepicker -->
        <!--<script src="/bundles/idklego/lib/datetimepicker/bootstrap-datetimepicker.min.js"></script>-->
        <!-- SlimScroll -->
        <script src="/bundles/idklego/adminLte/plugins/slimScroll/jquery.slimscroll.min.js"></script>
        <!-- FastClick -->
        <script src="/bundles/idklego/adminLte/plugins/fastclick/fastclick.js"></script>
        <!-- Wysihtml -->
        <script src="/bundles/idklego/adminLte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
        <!-- AdminLTE App -->
        <script src="/bundles/idklego/adminLte/dist/js/app.min.js"></script>
        <!-- Gridstack -->
        <script src="/bundles/idklego/lib/gridstack/gridstack.js"></script>
        <script src="/bundles/idklego/lib/gridstack/gridstack.jQueryUI.js"></script>
        <!-- Treetable -->
        <script src="/bundles/idklego/lib/treetable/jquery.treetable.js"></script>

        <!-- Jquery flip -->
        <script src="/bundles/idklego/lib/jquery.flip.js"></script>

        <!-- IDKcode -->
        <script src="/bundles/idklego/js/jsa.js"></script>
        <script src="/bundles/idklego/js/lego.js"></script>
    {%  endblock %}

</head>
<body class="hold-transition {{ lego_view.skin }} sidebar-mini">
    <div class="wrapper">

        {{ render(controller('IdkLegoBundle:Layout:header')) }}
        {{ render(controller('IdkLegoBundle:Layout:menu')) }}
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    {% block page_title %}{% endblock %}
                    <small>{% block page_subtitle %}{% endblock %}</small>
                </h1>
                {#<ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li><a href="#">Examples</a></li>
                    <li class="active">Blank page</li>
                </ol>#}
            </section>

            <!-- Main content -->
            <section class="content">
                {% block extra_actions_header %}{% endblock %}
                {% block content %}{% endblock %}

            </section>
            <!-- /.content -->
        </div>
        {{ render(controller('IdkLegoBundle:Layout:footer')) }}
        {% include '@IdkLego/Layout/_menu_right.html.twig' %}

    </div>



</body>
</html>
